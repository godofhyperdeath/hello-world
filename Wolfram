    com.wolframalpha.module([
        'com.wolframalpha.service.WAUserService'
    ], function(
        WAUserService
    ) {
      WAUserService.getUser().then(function handleLoggedInOut(user) {
        if(user === null) {
          script.src = "//www.wolframcdn.com/consent/cookie-consent.php";
          document.head.appendChild(script);
        }
        else {
          script.src = "//www.wolframcdn.com/consent/cookie-consent.php?signedin=true";
          document.head.appendChild(script);
        }
      });
    });
